# Ignore build outputs
dist/
build/
*.tgz

# Ignore test coverage
coverage/
.nyc_output/

# Ignore logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore IDE files
.vscode/
.idea/
*.swp
*.swo

# Ignore temporary files
*.tmp
*.temp

# Ignore package manager files
node_modules/
.pnpm-debug.log*

# Ignore environment files
.env
.env.local
.env.*.local

